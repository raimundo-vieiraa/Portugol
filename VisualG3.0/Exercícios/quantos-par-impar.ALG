algoritmo "n-par-e-impar"
var
   c, nimpar, npar: inteiro
inicio
      escreva("digite um numero: ")
      leia(npar)
      para c <- 2 ate npar passo 2 faca
           escreval(c)
      fimpara
      
      escreva("digite um número: ")
      leia(nimpar)
      para c <- 1 ate nimpar passo 2 faca
           escreval(c)
      fimpara
fimalgoritmo